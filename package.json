{
  "name": "inuit-boilerplate",
  "version": "1.0.0",
  "description": "A starter project for using the excellent InuitCSS framework",
  "repository": {
    "type": "git",
    "url": "git://github.com/john-cheesman/inuit-boilerplate.git"
  },
  "keywords": [
    "inuitcss",
    "inuit",
    "sass",
    "scss",
    "inuit",
    "css3"
  ],
  "author": "John Cheesman",
  "license": "MIT",
  "bugs": {
    "url": "git://github.com/john-cheesman/inuit-boilerplate.git"
  },
  "dependencies": {
    "inuitcss": "^6.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "clean-css-cli": "^4.3.0",
    "env-cmd": "^2.2.0",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.13.1",
    "postcss-cli": "^6.1.3",
    "rimraf": "^2.5.4",
    "runjs": "^2.5.1",
    "watch": "^1.0.1"
  },
  "scripts": {
    "build": "env-cmd .env run build",
    "serve": "env-cmd .env run serve",
    "watch": "env-cmd .env run watch",
    "add:component": "env-cmd .env run generate:component",
    "start": "env-cmd .env run start"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
